{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://0.0.0.0:5176"
            },
            "Https": {
                "Url": "https://0.0.0.0:7032"
            }
        }
    },
  "Database": {
    "Host": "${DB_HOST:-127.0.0.1}",
    "Port": "${DB_PORT:-5432}",
    "Name": "${DB_NAME:-wss_prod}",
    "User": "${DB_USER:-wss}",
    "Pass": "${DB_PASS:-WSS}"
  }
}
